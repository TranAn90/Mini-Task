<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .gwt-DialogBox .dialogMiddleCenter;
	.gwt-DialogBox .dialogMiddleCenter {
		padding: 0px !important;
	}
	
	.zoomOutDes_Obj2 {
		line-height: 27px !important;
	}
	</ui:style>
	<g:HTMLPanel ui:field="htmlZoomOutDescription" height="450px" width="100%">
		<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_LEFT"
			spacing="0" height="100%">
			<g:RichTextArea width="100%" height="400px"
				styleName="textbox-normalEdit" ui:field="description" />
			<g:AbsolutePanel width="100%" height="50px"
				styleName="background-softGray">
				<g:at left="5" top="10">
					<g:Label styleName="{style.zoomOutDes_Obj2} button-greenStyle"
						width="80px" height="30px" horizontalAlignment="ALIGN_CENTER"
						ui:field="btSave"></g:Label>
				</g:at>
				<g:at left="100" top="10">
					<g:Label styleName="{style.zoomOutDes_Obj2} button-whiteStyle"
						width="70px" height="30px" horizontalAlignment="ALIGN_CENTER"
						ui:field="btReload"></g:Label>
				</g:at>
				<g:at left="190" top="16">
					<g:Label ui:field="lbOr" />
				</g:at>
				<g:at left="225" top="16">
					<g:Anchor horizontalAlignment="ALIGN_CENTER" styleName="button-link"
						width="50px" ui:field="btCancel" />
				</g:at>
			</g:AbsolutePanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 