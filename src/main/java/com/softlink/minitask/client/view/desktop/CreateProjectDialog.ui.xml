<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
	.createProjectDialog_Obj1 {
		padding: 0 !important;
		width: 100%;
	}
	
	.createProjectDialog_Obj2 {
		line-height: 27px !important;
	}
	
	.createProjectDialog_Obj3 {
		font-weight: bold;
	}
	
	.createProjectDialog_Obj4 {
		margin-top: 5px;
	}
	</ui:style>
	<g:HTMLPanel styleName="frame-grayDialog" width="600px">
		<g:VerticalPanel styleName="{style.createProjectDialog_Obj1}">
			<g:AbsolutePanel width="100%" height="50px"
				styleName="background-softGray">
				<g:at left="12" top="15">
					<g:Label styleName="font-blackNormalHeader" ui:field="header" />
				</g:at>
				<g:at left="530" top="20">
					<g:Image width="22px" height="18px" url="resources/refresh.png" ui:field="btReload" />
				</g:at>
				<g:at left="560" top="20">
					<g:Image width="22px" height="18px" url="resources/close.png" ui:field="btClose" />
				</g:at>
			</g:AbsolutePanel>
			<g:ScrollPanel height="100%" width="100%">
				<g:VerticalPanel height="100%" width="100%">
					<g:AbsolutePanel height="160px">
						<g:at left="25" top="20">
							<g:Label styleName="font-blackTitleNormal"
								width="90px" ui:field="lbNameProject" />
						</g:at>
						<g:at left="117" top="12">
							<g:TextBox width="450px" styleName="textbox-normalEdit"
								height="20px" ui:field="name" />
						</g:at>
						<g:at left="25" top="60">
							<g:Label styleName="font-blackTitleNormal"
								width="90px" ui:field="lbManager" />
						</g:at>
						<g:at left="117" top="52">
							<g:ListBox visibleItemCount="0" width="200px"
								styleName="textbox-normalEdit" height="25px" ui:field="manager" />
						</g:at>
						<g:at left="347" top="60">
							<g:Label styleName="font-blackTitleNormal"
								width="60px" ui:field="lbDueDate" />
						</g:at>
						<g:at left="417" top="52">
							<p1:DateBox width="150px" styleName="textbox-normalEdit"
								height="20px" ui:field="dueDate" />
						</g:at>

						<g:at left="25" top="100">
							<g:Label styleName="font-blackTitleNormal"
								width="90px" ui:field="lbDescription" />
						</g:at>
						<g:at left="117" top="92">
							<g:RichTextArea width="448px" height="50px"
								styleName="textbox-normalEdit" ui:field="description" />
						</g:at>
						<g:at left="570" top="110">
							<g:Image width="20px" height="20px"
								url="resources/zoom_out.png" ui:field="btZoomOut" />
						</g:at>
					</g:AbsolutePanel>
					<g:DisclosurePanel open="true" width="100%"
						height="100%" ui:field="disclosurePanel">
						<g:header></g:header>

						<g:VerticalPanel width="100%" height="100%"
							spacing="5">
							<g:AbsolutePanel width="100%" height="40px">
								<g:at left="0" top="10">
									<g:Label styleName="font-blackTitleNormal" width="90px" ui:field="lbStartDate" />
								</g:at>
								<g:at left="300" top="10">
									<g:Label width="90px" styleName="font-blackTitleNormal" ui:field="lbEndDate" />
								</g:at>

								<g:at left="0" top="90">
									<g:Label text="Công việc cha" styleName="Csstask_lb"
										visible="false" />
								</g:at>
								<g:at left="96" top="2">
									<p1:DateBox width="150px" height="20px"
										styleName="textbox-normalEdit" ui:field="startDate" />
								</g:at>
								<g:at left="400" top="2">
									<p1:DateBox width="145px" height="20px"
										styleName="textbox-normalEdit" ui:field="endDate" />
								</g:at>




							</g:AbsolutePanel>
							<g:Anchor horizontalAlignment="ALIGN_CENTER" styleName="button-link" ui:field="btAddMember" />

							<g:FlowPanel width="90%" height="100%" ui:field="tableMember" />
						</g:VerticalPanel>


					</g:DisclosurePanel>
				</g:VerticalPanel>
			</g:ScrollPanel>
			<g:AbsolutePanel width="100%" height="40px"
				styleName="background-softGray">
				<g:at left="10" top="5">
					<g:Label styleName="{style.createProjectDialog_Obj2} button-greenStyle"
						width="60px" height="30px" horizontalAlignment="ALIGN_CENTER" ui:field="btSave"></g:Label>
				</g:at>
				<g:at left="85" top="5">
					<g:Label styleName="{style.createProjectDialog_Obj2} button-whiteStyle"
						width="100px" height="30px"
						horizontalAlignment="ALIGN_CENTER" ui:field="btSaveContinue"></g:Label>
				</g:at>
				<!-- <g:at left="232" top="20"> <g:Label visible="false">Hoặc</g:Label> 
					</g:at> <g:at left="268" top="16"> <g:Anchor horizontalAlignment="ALIGN_CENTER" 
					styleName="button_link" text="Hủy bỏ" ui:field="btCancel" visible="false" 
					/> </g:at> -->
			</g:AbsolutePanel>

		</g:VerticalPanel>

	</g:HTMLPanel>
</ui:UiBinder> 