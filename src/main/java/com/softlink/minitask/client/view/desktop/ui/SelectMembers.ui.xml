<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.SelectMembers-Obj1 {
		position: relative;
		float: left;
		top: 15px;
		left: 10px;
		text-align: center;
		line-height: 30px;
		margin-right: 10px;
	}
	
	.SelectMembers-Obj2 {
		background-color: white;
	}
	
	.SelectMembers-Obj3 {
		position: relative;
		float: right;
		top: 28px;
		right: 10px;
	}
	</ui:style>

	<g:VerticalPanel width="320px" height=""
		styleName="frame-grayDialog">
		<g:AbsolutePanel width="100%" height="40px"
			styleName="background-softGray">
			<g:at left="12" top="10">
				<g:Label styleName="font-blackTitle" text="Chọn thành viên" />
			</g:at>
			<g:at left="268" top="10">
				<g:Image width="22px" height="18px" url="resources/close.png"
					title="Đóng" ui:field="imgClose" />
			</g:at>
		</g:AbsolutePanel>
		<g:ScrollPanel height="230px" styleName="{style.SelectMembers-Obj2}">
			<p1:CellTable ui:field="talbeUser" width="100%" height="100%" />
		</g:ScrollPanel>
		<g:HTMLPanel height="60px" width="100%" styleName="background-softGray">
			<g:Label text="Đồng ý" width="85px"
				styleName="{style.SelectMembers-Obj1} button-greenStyle" ui:field="btnOk" height="" />
			<g:Label text="Hủy" styleName="{style.SelectMembers-Obj1} button-whiteStyle"
				width="76px" ui:field="btnCancel" height="" />
			<g:Label text="Chọn tất" width=""
				styleName="{style.SelectMembers-Obj3} button-link" ui:field="btnSelectAllMember" />
		</g:HTMLPanel>
	</g:VerticalPanel>



</ui:UiBinder> 