	<ui:style>
	.SlideBar-Obj1 {
		text-align: center;
		line-height: 45px;
		cursor: pointer;
		border-radius: 0px;
		border-bottom: 1px silver solid;
		background: #48b512;
	}
	</ui:style>
	<g:HTMLPanel styleName="background-softBlue" width="80px" height="100%">
		<g:Label height="45px" styleName="{style.SlideBar-Obj1} button-greenStyle" text="Dự án"/>
		<g:Label text="Công việc" styleName="{style.SlideBar-Obj1} button-greenStyle" height="45px"/>
	</g:HTMLPanel>
</ui:UiBinder> 