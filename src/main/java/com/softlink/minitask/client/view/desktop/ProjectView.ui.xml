<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.ProjectDetail-Obj1 {
		width: 100%;
	}
	
	.ProjectDetail-Object2 {
		width: 22%;
		vertical-align: top;
		text-align: left;
		margin-left: 0px;
	}
	
	.ProjectDetail-Object3 {
		width: 28%;
		vertical-align: top;
	}
	
	.ProjectDetail-Object4 {
		background: #ffffdd;
		padding: 6px;
		margin-top: 10px;
		margin-bottom: 6px;
		border: 1px solid #d7d7d7;
	}
	
	.ProjectDetail-Object5 {
		margin-left: 5px;
		padding-top: 0px !important;
	}
	
	.ProjectDetail-Object6 {
		position: relative;
		float: left;
		width: 70%;
		margin-left: 10px;
		margin-right: 10px;
	}
	
	.ProjectDetail-Object10 {
		float: right;
	}
	
	.ProjectDetail-Object9 {
		line-height: 22px !important;
		margin-left: 5px;
	}
	
	.ProjectDetail-Object11 {
		margin-left: 5px;
		padding-bottom: 5px;
	}
	
	.ProjectDetail-Object12 {
		float: left;
		position: relative;
		left: 15px;
		top: 13%;
		height: 70%;
	}
	
	.ProjectDetail-Object13 {
		float: right;
		position: relative;
		right: 20px;
		top: 13%;
		height: 70%;
	}
	
	.ProjectDetail-Object14 {
		line-height: 30px;
		text-align: center;
		margin-right: 10px;
	}
	
	.ProjectDetil-Object15 {
		float: right;
		white-space: nowrap;
		line-height: 1.4em;
		margin-top: 5px;
		padding-left: 10px;
		font-size: 0.9em;
	}
	
	.ProjectDetil-Object16 {
		margin-left: 5px;
		padding-top: 10px;
	}
	</ui:style>

	<g:HTMLPanel>
		<g:HTMLPanel height="46px" styleName="background-softGray"
			ui:field="htmlMenu">
			<g:HorizontalPanel styleName="{style.ProjectDetail-Object12}">
				<g:Label text="Quay láº¡i" width="65px" height="100%"
					styleName="{style.ProjectDetail-Object14} button-whiteStyle"
					ui:field="btnBack" />
			</g:HorizontalPanel>
			<g:HorizontalPanel styleName="{style.ProjectDetail-Object13}" />
		</g:HTMLPanel>

		<g:ScrollPanel ui:field="sclContent">
			<g:HTMLPanel>
				<g:VerticalPanel styleName="{style.ProjectDetail-Object6}">

					<g:VerticalPanel width="100%"
						styleName="{style.ProjectDetail-Object4}" ui:field="verMainContent">
						<g:HTML styleName="{style.ProjectDetail-Object5}">
							<div>
								<h3 style="margin-top: 0px; font-style: italic;" ui:field="name">
								</h3>
							</div>
						</g:HTML>


						<g:HTML>
							<table class="{style.ProjectDetail-Obj1}">
								<tr>
									<th class="{style.ProjectDetail-Object2}" ui:field="managerth" />
									<td class="{style.ProjectDetail-Object3}" ui:field="manager">
									</td>
									<th class="{style.ProjectDetail-Object2}" ui:field="startDateth">
									</th>
									<td class="{style.ProjectDetail-Object3}" ui:field="startDate" />
								</tr>
								<tr>
									<th class="{style.ProjectDetail-Object2}" ui:field="creatorth"></th>
									<td class="{style.ProjectDetail-Object3}" ui:field="creator"></td>
									<th class="{style.ProjectDetail-Object2}" ui:field="dueDateth"></th>
									<td class="{style.ProjectDetail-Object3}" ui:field="dueDate"></td>
								</tr>
								<th class="{style.ProjectDetail-Object2}" ui:field="listMemberth"></th>
								<td class="{style.ProjectDetail-Object3}" ui:field="listMember">
								</td>
								<tr>
								</tr>
							</table>
							<hr></hr>
						</g:HTML>
						<g:HTML>
							<div class="{style.ProjectDetail-Object5}">
								<strong ui:field="descriptionth" style=""></strong>
								<div>
									<p ui:field="description"></p>
								</div>
							</div>
							<hr />
						</g:HTML>
						<g:HTMLPanel styleName="{style.ProjectDetail-Object5}">
							<g:Anchor horizontalAlignment="ALIGN_CENTER" styleName="button-link"
								addStyleNames="{style.ProjectDetil-Object15}" ui:field="btNewTask" />
							<h3 style="margin-top: 0px; font-style: italic;" ui:field="nameSubProject" />
						</g:HTMLPanel>
						<g:HTML styleName="{style.ProjectDetail-Object5}">
							<table class="{style.ProjectDetail-Obj1}">
								<tr>
									<th class="{style.ProjectDetail-Object2}" ui:field="initDateSubth">ngay tao</th>

									<td class="{style.ProjectDetail-Object3}" ui:field="initDateSub">
										21/01/1990
									</td>
									<th class="{style.ProjectDetail-Object2}" ui:field="dueDateSubth"> Due date
									</th>
									<td class="{style.ProjectDetail-Object3}" ui:field="dueDateSub">hehe</td>

								</tr>
							</table>
						</g:HTML>
						<g:HTMLPanel styleName="{style.ProjectDetil-Object16}">
							<p1:CellTable ui:field="gridTasks"></p1:CellTable>
							<hr></hr>
						</g:HTMLPanel>

					</g:VerticalPanel>

				</g:VerticalPanel>

			</g:HTMLPanel>
		</g:ScrollPanel>
	</g:HTMLPanel>

</ui:UiBinder> 