<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.UserProfileDialog-Obj1 {
		float: right;
		text-indent: -22px;
		opacity: 0.5;
		cursor: pointer;
		font-size: 16px;
		line-height: 18px;
	}
	
	.UserProfileDialog-Obj1:hover {
		opacity: 1;
	}
	
	.UserProfileDialog-Obj2 {
		text-align: center;
		font-size: 16px;
	}
	
	.UserProfileDialog-Obj3 {
		text-indent: 20px;
		font-size: 14px;
		cursor: pointer;
		line-height: 25px;
		margin-top: 14px;
	}
	
	.UserProfileDialog-Obj3:hover {
		background: #0e74af;
		color: #f6f6f6;
	}
	
	.UserProfileDialog-Obj4 {
		margin: auto;
		border-top: 1px #c2c2c2;
		border-right: 1px #c2c2c2;
		border-bottom: 1px solid #c2c2c2;
		border-left: 1px #c2c2c2;
	}
	</ui:style>
	<g:HTMLPanel styleName="dialog-menu" width="275px" height="95px">
		<g:AbsolutePanel height="10px" width="100%"/>
		<g:Label text="x" styleName="{style.UserProfileDialog-Obj1} font-blackTitle" ui:field="btnClose"/>
		<g:Label text="Trần An" ui:field="lbUserName" styleName="{style.UserProfileDialog-Obj2} font-grayTitle"/>
		<g:AbsolutePanel height="10px" width="90%" styleName="{style.UserProfileDialog-Obj4}"/>
		<g:Label text="Đăng xuất" styleName="{style.UserProfileDialog-Obj3} font-blackTitle" height="25px" ui:field="btnSignOut"/>
	</g:HTMLPanel>
</ui:UiBinder> 